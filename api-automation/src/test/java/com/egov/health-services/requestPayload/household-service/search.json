{
    "searchWithId": {
        "RequestInfo": {
            "authToken": "#(hcmAuthToken)"
        },
        "Household": {
            "id": [
                "#(idOfHousehold)"
            ]
        }
    },
    "searchWithClientReferenceId": {
        "RequestInfo": {
            "authToken": "#(hcmAuthToken)"
        },
        "Household": {
            "clientReferenceId": [
                "#(clientReferenceIdOfHousehold)"
            ]
        }
    },
    "searchWithMultipleClientReferenceId": {
        "RequestInfo": {
            "authToken": "#(hcmAuthToken)"
        },
        "Household": {
            "clientReferenceId": #(clientReferenceIdOfHousehold)
        }
    },
    "searchWithBoundaryCode": {
        "RequestInfo": {
            "authToken": "#(hcmAuthToken)"
        },
        "Household": {
            "boundaryCode": "#(boundaryCodeOfHousehold)"
        }
    },
    "searchWithAllCriteria": {
        "RequestInfo": {
            "authToken": "#(hcmAuthToken)"
        },
        "Household": {
            "id": [
                "#(idOfHousehold)"
            ],
            "clientReferenceId": [
                "#(clientReferenceIdOfHousehold)"
            ],
            "boundaryCode": "#(boundaryCodeOfHousehold)"
        }
    }
}